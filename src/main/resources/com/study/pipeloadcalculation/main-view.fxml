<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.study.pipeloadcalculation.controller.MyAppController">
   <children>
      <VBox minWidth="400.0">
         <children>
            <Label fx:id="packingDetails" prefHeight="26.0" prefWidth="346.0" text="info">
               <font>
                  <Font size="17.0" />
               </font>
            </Label>
            <Button fx:id="deleteCurrentVariationButton" mnemonicParsing="false" onAction="#onButtonDeleteCurrent" text="Delete" />
            <HBox prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
               <children>
                  <ListView fx:id="listViewVariations" onMouseClicked="#onVariationClicked" prefHeight="200.0" prefWidth="200.0" />
                  <ListView fx:id="deletedVariationsList" onMouseClicked="#onVariationClicked" prefHeight="200.0" prefWidth="200.0" />
               </children>
            </HBox>
         </children>
      </VBox>
      <Pane fx:id="drawPane" minHeight="400.0" minWidth="400.0" HBox.hgrow="SOMETIMES" />
   </children>
</HBox>
